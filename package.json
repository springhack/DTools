{
	"name": "DTools",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "react-native run-android",
		"init": "npm install --registry=https://registry.npm.taobao.org",
		"lint": "find src *.js | grep \\.js$ | xargs eslint",
		"lint:fix": "find src *.js | grep \\.js$ | xargs eslint --fix",
		"build:apk": "cd android && ./gradlew assembleRelease",
		"build:install": "cd android && ./gradlew installRelease",
		"start": "node node_modules/react-native/local-cli/cli.js start"
	},
	"dependencies": {
		"antd-mobile": "^1.3.1",
		"mobx": "^3.1.16",
		"mobx-react": "^4.2.1",
		"moment": "^2.18.1",
		"react": "16.0.0-alpha.12",
		"react-native": "0.45.1",
		"react-navigation": "^1.0.0-beta.11",
		"react-router-native": "^4.1.1"
	},
	"devDependencies": {
		"babel-eslint": "^7.2.3",
		"babel-jest": "20.0.3",
		"babel-plugin-import": "^1.2.1",
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-preset-react-native": "2.0.0",
		"babel-preset-stage-0": "^6.24.1",
		"eslint": "^3.19.0",
		"eslint-config-airbnb": "^15.0.1",
		"eslint-plugin-import": "^2.6.0",
		"eslint-plugin-jsx-a11y": "^5.0.3",
		"eslint-plugin-react": "^7.1.0",
		"jest": "20.0.4",
		"react-test-renderer": "16.0.0-alpha.12"
	},
	"jest": {
		"preset": "react-native"
	}
}
